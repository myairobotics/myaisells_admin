{
  "name": "MyaiSells",
  "author": "VinJex",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "next dev",
    "spotlight": "npx @spotlightjs/spotlight",
    "dev:spotlight": "pnpm run -r run-p dev spotlight",
    "build": "next build",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "rimraf .next out coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check:types": "tsc --noEmit --pretty",
    "check:deps": "knip",
    "check:i18n": "i18n-check -l src/locales -s en -u src -f next-intl",
    "commit": "commit",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "storybook": "storybook dev -p 6006",
    "storybook:test": "vitest run --config .storybook/vitest.config.mts",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@arcjet/next": "^1.0.0-beta.13",
    "@commitlint/types": "^20.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.2",
    "@logtape/logtape": "^1.2.0",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@reduxjs/toolkit": "^2.10.1",
    "@sentry/nextjs": "^10.26.0",
    "@svgr/webpack": "^8.1.0",
    "@t3-oss/env-nextjs": "^0.13.8",
    "ag-charts-react": "^12.3.1",
    "import-in-the-middle": "2.0.0",
    "next": "^16.0.3",
    "next-auth": "5.0.0-beta.30",
    "next-intl": "^4.5.5",
    "posthog-js": "^1.297.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.1",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-toastify": "^11.0.5",
    "require-in-the-middle": "^8.0.1",
    "vite": "^7.2.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@chromatic-com/playwright": "^0.12.7",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/prompt-cli": "^20.1.0",
    "@eslint-react/eslint-plugin": "^2.3.7",
    "@faker-js/faker": "^10.1.0",
    "@lingual/i18n-check": "^0.8.14",
    "@next/bundle-analyzer": "^16.0.3",
    "@next/eslint-plugin-next": "^16.0.3",
    "@playwright/test": "^1.56.1",
    "@spotlightjs/spotlight": "^4.5.1",
    "@storybook/addon-a11y": "^10.0.8",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/addon-vitest": "^10.0.8",
    "@storybook/nextjs-vite": "^10.0.8",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser": "^4.0.13",
    "@vitest/browser-playwright": "^4.0.13",
    "@vitest/coverage-v8": "^4.0.13",
    "babel-plugin-react-compiler": "^1.0.0",
    "checkly": "^6.9.2",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.39.0",
    "eslint-plugin-format": "^1.0.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.3.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.0.8",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "knip": "^5.69.1",
    "lefthook": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "rimraf": "^6.1.2",
    "semantic-release": "^25.0.2",
    "storybook": "^10.0.8",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.13",
    "vitest-browser-react": "^2.0.2"
  },
  "pnpm": {
    "overrides": {
      "import-in-the-middle": "2.0.0",
      "require-in-the-middle": "8.0.1"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
